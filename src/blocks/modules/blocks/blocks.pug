mixin elem(elemTitle, elemText, elemBtn)
	.elem(class=attributes.class)
		.elem__body 
			.elem__head 
				img.elem__icon(src="./img/icon.svg", alt="icon")
				h3= elemTitle
			.elem__content
					p= elemText
					a.elem__button(href="#")= elemBtn



